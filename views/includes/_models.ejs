<div id="AddSubCategory" class="modal fade" tabindex="-1" role="dialog">
  <div class="modal-dialog   modal-sm">
    <div class="modal-content">
      <div class="modal-header bg-primary">
        <button type="button" class="close" data-dismiss="modal">
          <span aria-hidden="true">×</span>
          <span class="sr-only">Close</span>
        </button>
        <div class="text-center">
          <h4 class="modal-title">Add Sub Category</h4>
        </div>
      </div>
      <div class="modal-body">
        <div class="text-center">
          <h3 class="text-primary"></h3>
          <form method="POST" id="AddSubCategory" name="addSubCategory" enctype="multipart/form-data">
            <div class="form-group">
              <label class="control-label">Sub Category Name</label>
              <input class="form-control" type="text" id="subCategoryName" name="subCategoryName" />
            </div>
            <div class="form-group">
              <label class="control-label">Under Category</label>

              <select id="inputState" name="subCategoryId" class="form-control">
                <% if (category.length=="0") { %>
                <option> ----EMPTY----- </option>
                <% } %> <% category.forEach(Category=>{ %>
                <option value="<%=Category.id %>"><%=Category.categoryName %></option>
                <% }) %>
              </select>
            </div>
            <div class="form-group">
              <label class="control-label">Status</label>
              <select id="statusId" name="status" class="form-control">
                <option value="true">Active</option>
                <option value="false">InActive</option>
              </select>
            </div>
            <div class="m-t-lg">
              <input class="btn btn-primary addSubCategory" type="submit" value="save" />
              <button class="btn btn-default" data-dismiss="modal" type="button">Cancel</button>
            </div>
          </form>
        </div>
      </div>
      <div class="modal-footer"></div>
    </div>
  </div>
</div>
